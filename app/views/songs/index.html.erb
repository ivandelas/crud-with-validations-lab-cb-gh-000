<%= @songs.each do |song| %>
<table>
      <thead>
        <tr>
          <th>Title</th>
          <th>Artist Name</th>
          <th>Genre</th>
          <th>Year</th>
          <th colspan="3"></th>
        </tr>
      </thead>
       <tbody>
        <tr>
          <td><%= song.title %></td>
          <td><%= song.artist_name %></td>
          <td><%= song.genre %></td>
          <td><%= song.release_year %></td>
          <td><%= link_to "Show", song %></td>
          <td><%= link_to "Edit", edit_song_path(song) %></td>
          <td><%= link_to "Delete", song, method: :delete, data: { confirm: "Are you sure?" } %></td>
          <td><%= link_to 'New', new_song_path(song) %></td>
        </tr>
      </tbody>
    </table>
<% end %>
